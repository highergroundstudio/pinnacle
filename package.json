{
	"name": "lenderify",
	"version": "5.0.0",
	"private": true,
	"files": [
		"dist"
	],
	"svelte": "dist/index.js",
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	},
	"scripts": {
		"dev": "vite dev",
		"release": "release-it && npm run docker-push",
		"updatedeps": "npx taze -w && npm install",
		"dev:network": "vite --host",
		"build": "vite build",
		"start": "export PORT=3000 && node ./build",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui --api 9527",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"docker-push-version": "docker build --tag highergroundstudio/lenderify:v%npm_package_version% . && docker push highergroundstudio/lenderify:v%npm_package_version%",
		"docker-push-latest": "docker build --tag highergroundstudio/lenderify . && docker push highergroundstudio/lenderify",
		"docker-push": "npm run docker-push-version && npm run docker-push-latest"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.2",
		"@skeletonlabs/skeleton": "^2.10.2",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.13",
		"@types/google.maps": "^3.55.12",
		"@types/node": "^20.14.13",
		"@types/qs": "^6.9.15",
		"@vitest/coverage-v8": "^2.0.4",
		"@vitest/ui": "^2.0.4",
		"autoprefixer": "^10.4.19",
		"chalk": "^5.3.0",
		"listr2": "^8.2.4",
		"postcss": "^8.4.40",
		"postcss-load-config": "^6.0.1",
		"progress-barjs": "^2.2.1",
		"qs": "^6.12.3",
		"release-it": "^17.6.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-multiselect": "^10.3.0",
		"svelte-preprocess": "^6.0.2",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.16.1",
		"tailwindcss": "^3.4.7",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vite-plugin-tailwind-purgecss": "^0.3.3",
		"vitest": "^2.0.4",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.16.8",
		"@hubspot/api-client": "^11.1.0",
		"@neoconfetti/svelte": "^2.2.1",
		"pdf-lib": "^1.17.1"
	}
}